{
    "name": "softexpert/softexpert",
    "description": "The softexpert App.",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "symfony/dotenv": "5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Bootstrap\\": "bootstrap/"
        },
        "files": [
            "app/Support/Helpers/Global.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
